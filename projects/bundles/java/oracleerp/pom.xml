<?xml version="1.0" encoding="UTF-8"?>
<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd"
	xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.forgerock.openicf.bundles</groupId>
        <artifactId>bundles-parent</artifactId>
        <version>1.1.0-SNAPSHOT</version>
        <relativePath>../../maven/bundles-parent</relativePath>
    </parent>
    <artifactId>oracleerp</artifactId>
    <packaging>jar</packaging>
    <name>OracleERP Connector</name>
    <description>Connector for OracleERP</description>

    <properties>
        <skipTests>true</skipTests>
        <contracts.skip>true</contracts.skip>
        <embedded.dependencies>dbcommon,ojdbc14</embedded.dependencies>
        <connectorName>org.identityconnectors.oracleerp.OracleERPConnector</connectorName>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.forgerock.openicf.bundles</groupId>
            <artifactId>dbcommon</artifactId>
            <version>1.1.0-SNAPSHOT</version>
        </dependency>
        <!--
        Download from http://www.oracle.com/technetwork/database/enterprise-edition/jdbc-10201-088211.html
        mvn install:install-file -Dfile=ojdbc14.jar -DgroupId=com.oracle -DartifactId=ojdbc14 -Dversion=10.2.0.5 -Dpackaging=jar -DgeneratePom=true
        -->
        <dependency>
            <groupId>com.oracle</groupId>
            <artifactId>ojdbc14</artifactId>
            <version>10.2.0.5</version>
        </dependency>
    </dependencies>

</project>
